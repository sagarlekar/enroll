/* ==========================================================================
   footer
   ========================================================================== */

footer {
  position: absolute;
  bottom: 0;
  width: 100%;
  /* Set the fixed height of the footer here */
  z-index : 1000;
  height: 2px;
}

/* ==========================================================================
   footer / footer-1
   ========================================================================== */

.footer-1{
  background-color: #ffffff;
  padding-top: 30px;
  padding-bottom: 30px;
}
.footer-1 ul.link-1 li{
  display: inline;
}
ul.link-1{
  font-size: 12px;
  font-weight: 700;
  margin-top: 20px;
}
ul.link-1 li{
  border-right: 1px solid #008ecd;
  padding-right: 10px;
  padding-left: 10px;
}
ul.link-1 li a:hover{
  text-decoration: none;
}
ul.link-1 li:first-child{
  padding-left: 0px;
}
ul.link-1 li:last-child{
  padding-right: 0px;
  border-right: 0px;
}


ul.link-2{
  font-size: 14px;
  display: inline;
  padding-left: 0;
}


ul.link-2{
  list-style: none;
}
ul.link-2 li.inline{
  display: inline-block;
  font-size: 12px;
  font-weight: 700;
}
